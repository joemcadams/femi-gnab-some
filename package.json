{
  "name": "femi-gnab-some",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "node-sass": "^4.5.0",
    "react-scripts": "0.9.3"
  },
  "dependencies": {
    "compromise": "^7.0.28",
    "concurrently": "^3.4.0",
    "express": "^4.15.2",
    "json-format": "^1.0.0",
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "mongodb": "^2.2.25",
    "path": "^0.12.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-highcharts": "^11.5.0",
    "react-icons": "^2.2.3",
    "react-scripts": "0.9.3",
    "sentiment": "^3.0.1",
    "twit": "^2.2.5"
  },
  "scripts": {
    "build-css": "node-sass src/styles/index.scss -o src/styles/",
    "bundle": "concurrently \"react-scripts start\" \"npm run watch-css\"",
    "watch-css": "node-sass src/styles/index.scss -o src/styles/ --watch",
    "start": "npm run start-server",
    "start-development": "set NODE_ENV=development && concurrently \"npm run bundle\" \"npm run start-server\"",
    "start-server": "node server",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
